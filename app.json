{
  "expo": {
    "name": "WisEnergy",
    "slug": "WisEnergy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.storeblues123.WisEnergy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WisEnergy_LOGO1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.storeblues123.WisEnergy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b2e2304-e953-495b-ac21-c86deaabb1f7"
      }
    },
    "owner": "storeblues123"
  }
}